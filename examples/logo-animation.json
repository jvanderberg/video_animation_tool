{
  "project": {
    "width": 1920,
    "height": 1080,
    "fps": 30,
    "frames": 180
  },
  "objects": [
    {
      "type": "rect",
      "id": "background",
      "x": 0,
      "y": 0,
      "width": 1920,
      "height": 1080,
      "fill": "#1a1a1a"
    },
    {
      "type": "image",
      "id": "logo",
      "source": "../assets/logo.png",
      "x": 960,
      "y": 540,
      "anchor": "center",
      "scale": 0,
      "opacity": 0
    },
    {
      "type": "text",
      "id": "title",
      "content": "Animation Tool",
      "x": 960,
      "y": 720,
      "size": 64,
      "color": "#FFFFFF",
      "font": "Arial",
      "anchor": "center",
      "opacity": 0
    }
  ],
  "animations": [
    {
      "target": "logo",
      "effect": "pop",
      "start": "0.5s"
    },
    {
      "target": "logo",
      "property": "rotation",
      "keyframes": [
        {
          "start": 45,
          "value": 0
        },
        {
          "start": 105,
          "value": 360,
          "easing": "ease-in-out"
        }
      ]
    },
    {
      "target": "title",
      "effect": "fadeIn",
      "start": "1.5s"
    },
    {
      "target": "logo",
      "property": "y",
      "keyframes": [
        {
          "start": 105,
          "value": 540
        },
        {
          "start": 135,
          "value": 440,
          "easing": "ease-out"
        }
      ]
    },
    {
      "target": "title",
      "property": "y",
      "keyframes": [
        {
          "start": 105,
          "value": 720
        },
        {
          "start": 135,
          "value": 620,
          "easing": "ease-out"
        }
      ]
    }
  ]
}